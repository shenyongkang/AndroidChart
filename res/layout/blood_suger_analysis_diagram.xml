<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/bloodSugerAnalysisDiagramChart1"
        android:layout_width="180sp"
        android:layout_height="180sp"
        />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/bloodSugerAnalysisDiagramChart2"
        android:layout_width="220sp"
        android:layout_height="180sp"
        android:layout_toEndOf="@id/bloodSugerAnalysisDiagramChart1"
        android:layout_toRightOf="@id/bloodSugerAnalysisDiagramChart1" />

    <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/bloodSugerAnalysisDiagramChart3"
        android:layout_width="200dp"
        android:layout_height="200dp" 
        android:layout_below="@id/bloodSugerAnalysisDiagramChart1"/>
    

</RelativeLayout>
